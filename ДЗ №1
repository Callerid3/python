import random
# числа рандома от / до
print("Введите число 1")
a=int(input())
print("Введите число 2")
b=int(input())
print("Введите число 3")
c=int(input())
#
#рандомные числа
d=random.randint(1,1000)
print(d)
e=random.randint(1,1000)
print(e)
# условие для свапа d <> e
if d > e:
    f=e
    e=d
    d=f
# определения вхождения в список
if a > d:
    if a < e:
        print("Число", a, "входит в отрезок")
if b > d:
    if b < e:
        print("Число", b, "входит в отрезок")
if c > d:
    if c < e:
        print("Число", c, "входит в отрезок")
